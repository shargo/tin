program = expression+
expression = <line_start>? <lparen> form+ <rparen> | <line_start> symbol_or_number+
<form> = symbol_or_number | expression
<symbol_or_number> = symbol | number
symbol = #'[-+=!$%^&*_|\p{L}][-+=!$%^&*_|\p{L}\p{N}]*'
number = #'[\d]+'
<lparen> = <'('>
<rparen> = <')'>
<line_start> = <'≈'>
<indent> = <'»'>
<dedent> = <'«'>
